<Page x:Class="insoles.Graphs.GraphSumPressures"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:insoles.Graphs"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="GraphWindow">

    <Grid>
        <WpfPlot  x:Name="plot"/>
        <ComboBox Name="metric" SelectedValuePath="Content" VerticalAlignment="Top" HorizontalAlignment="Left">
            <ComboBoxItem Content="avg"></ComboBoxItem>
            <ComboBoxItem Content="sum"></ComboBoxItem>
        </ComboBox>
    </Grid>
</Page>

